# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration (comma-separated list of allowed origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:19006,exp://192.168.1.100:19000

# RapidAPI TripAdvisor Configuration
RAPIDAPI_KEY=your_rapidapi_key_here
TRIPADVISOR_RAPIDAPI_HOST=tripadvisor16.p.rapidapi.com
TRIPADVISOR_BASE_URL=https://tripadvisor16.p.rapidapi.com

# Default Location Settings
DEFAULT_CITY="Bucharest, Romania"
DEFAULT_LOCATION_ID="294458"

# Activities Provider Configuration
ACTIVITIES_PROVIDER=mock
PROVIDER_TIMEOUT=10000
PROVIDER_RETRY_ATTEMPTS=2
PROVIDER_CACHE_ENABLED=true
DEFAULT_CURRENCY=EUR
DEFAULT_LANGUAGE=en

# Feature Flags for Providers
FEATURE_REAL_TIME_PROVIDERS=false
FEATURE_POIS=false
FEATURE_EVENTS=false
FEATURE_BOOKING=false
FEATURE_MULTI_PROVIDER=false

# Viator (TripAdvisor Experiences) Configuration
VIATOR_API_KEY=your_viator_api_key_here
VIATOR_PARTNER_ID=your_viator_partner_id_here
VIATOR_BASE_URL=https://api.viator.com
VIATOR_TIMEOUT=15000
VIATOR_RATE_LIMIT=60
VIATOR_DAILY_LIMIT=10000
VIATOR_BOOKING_ENABLED=false
VIATOR_CURRENCY=USD
VIATOR_LANGUAGE=en

# GetYourGuide Configuration
GYG_API_KEY=your_getyourguide_api_key_here
GYG_PARTNER_ID=your_getyourguide_partner_id_here
GYG_BASE_URL=https://api.getyourguide.com
GYG_TIMEOUT=12000
GYG_RATE_LIMIT=100
GYG_DAILY_LIMIT=5000
GYG_BOOKING_ENABLED=false
GYG_CURRENCY=EUR
GYG_LANGUAGE=en

# Musement Configuration
MUSEMENT_API_KEY=your_musement_api_key_here
MUSEMENT_BASE_URL=https://api.musement.com
MUSEMENT_TIMEOUT=10000
MUSEMENT_RATE_LIMIT=120
MUSEMENT_DAILY_LIMIT=8000
